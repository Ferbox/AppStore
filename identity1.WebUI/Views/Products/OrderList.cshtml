@model IEnumerable<identity1.Common.Entities.Product>

<table class="table">
    <tr>
        <th>

        </th>
        <th>
            @Html.DisplayNameFor(model => model.Title)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CostProduct)
        </th>
        <th>
            Qty
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                <img src="@item.Images.FirstOrDefault(x => x.Name == "logo").Path" alt="Alternate Text" />
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Title)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CostProduct)
            </td>
            <td>
                <input type="text" name="qty" value="1" />
            </td>
            <td></td>
        </tr>
    }

</table>
